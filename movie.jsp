<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>Insert title here</title>

<script src="lib/jquery.js"></script>
<script>
 $(function() {
  // jquery 시작
  $.ajax({
   type : "get",    // get과 post가 있다
   url : "movie.json", // 접속하는 파일명
   datatype: "json"  // html, json ...(생략가능)
  }).done(function(data) {
   var movieHtmlStr = "";
   $("#dataList").empty();
   $.each(data, function(idx, val) {
    movieHtmlStr += "<tr>";
    movieHtmlStr += '<td>'+ val.name +'</td>';
    movieHtmlStr += '<td>'+ val.director +'</td>';
    movieHtmlStr += '<td>'+ val.actor +'</td>';
    movieHtmlStr += "</tr>";
   });
   $("#dataList").html(movieHtmlStr);
  });
 });
</script>
</head>
<body>

 <div id="result">
  <table border="1px solid black">
   <tr>
    <th>제목</th>
    <th>감독</th>
    <th>배우</th>
   </tr>
   <tbody id="dataList">

   </tbody>
  </table>
 </div>
 
</body>
</html>